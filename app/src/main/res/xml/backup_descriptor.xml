        domain="sharedpref"
        path="filename.xml" />
    <exclude
        domain="sharedpref"
        path="PREFS.xml" />
    <exclude
        domain="sharedpref"
        path="SP_USER.xml" />
    <exclude
        domain="sharedpref"
        path="prefs.xml" />
</full-backup-content>
